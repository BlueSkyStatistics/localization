          "visible": true
        },
        {
          "name": "Guardar conjunto de datos",
          "icon": "fas fa-save",
          "onclick": "saveDataset()",
          "visible": true
        },
        {
          "name": "Guardar como",
          "icon": "fas fa-file-export",
          "onclick": "saveasDataset()",
          "visible": true
        },
        {
          "name": "Cerrar conjunto de datos",
          "icon": "fas fa-times",
          "onclick": "closeDataset()",
          "visible": true
        },
        {
          "name": "Exportar salida",
          "icon": "fas fa-file-export",
          "onclick": "exportOutput()",
          "visible": true
        }
      ]
    },
    {
      "icon": "fas fa-ellipsis-v",
      "tab": "tools",
      "buttons": [
